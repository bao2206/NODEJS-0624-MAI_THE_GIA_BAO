<header id="header" class="dark transparent-header floating-header" data-sticky-shrink="false" data-sticky-class="not-dark">
  <div id="header-wrap">
    <div class="container-fluid">
      <div class="header-row justify-content-lg-between">

        <!-- Logo
        ============================================= -->
        <div id="logo" class="me-lg-0">
          <a href="/">
            <!-- <img class="logo-default" srcset="/frontend/dist/images/logo.png, /frontend/dist/images/logo@2x.png 2x" src="/frontend/dist/images/logo@2x.png" alt="Canvas Logo">
            <img class="logo-dark" srcset="/frontend/dist/images/logo-dark.png, /frontend/dist/images/logo-dark@2x.png 2x" src="/frontend/dist/images/logo-dark@2x.png" alt="Canvas Logo">
             -->
            <img src="/uploads/settings/<%= setting.image %>" class="Logo-default">
          </a>
        </div><!-- #logo end -->

        <div class="header-misc">

          <!-- Top Login
          ============================================= -->
          <div id="top-account" class="px-4">
            <a href="#modal-register" data-lightbox="inline">Sign up/Login</a>
          </div><!-- #top-search end -->

          <!-- Top Cart
          ============================================= -->
          <div id="top-cart">
            <a href="#" id="top-cart-trigger" class="position-relative"><i class="bi-bag"></i><span class="top-cart-number">5</span></a>
            <div class="top-cart-content">
              <div class="top-cart-title">
                <h4>Shopping Cart</h4>
              </div>
              <div class="top-cart-items">
                <div class="top-cart-item">
                  <div class="top-cart-item-image">
                    <a href="#"><img src="/frontend/dist/images/items/featured/5.jpg" alt="Blue Shoulder Bag"></a>
                  </div>
                  <div class="top-cart-item-desc">
                    <div class="top-cart-item-desc-title">
                      <a href="#" class="fw-normal">White athletic shoe</a>
                      <span class="top-cart-item-price d-block">$35.00</span>
                    </div>
                    <div class="top-cart-item-quantity fw-semibold">x 1</div>
                  </div>
                </div>
                <div class="top-cart-item">
                  <div class="top-cart-item-image">
                    <a href="#" class="fw-normal"><img src="/frontend/dist/images/items/featured/1.jpg" alt="Leather Bag"></a>
                  </div>
                  <div class="top-cart-item-desc">
                    <div class="top-cart-item-desc-title">
                      <a href="#" class="fw-normal">Round Neck Solid Light Blue Colour T-shirts</a>
                      <span class="top-cart-item-price d-block">$12.49</span>
                    </div>
                    <div class="top-cart-item-quantity fw-semibold">x 2</div>
                  </div>
                </div>
              </div>
              <div class="top-cart-action">
                <span class="top-checkout-price fw-semibold text-dark">$59.98</span>
                <button class="button button-dark button-small m-0">View Cart</button>
              </div>
            </div>
          </div><!-- #top-cart end -->
        </div>

        <div class="primary-menu-trigger">
          <button class="cnvs-hamburger" type="button" title="Open Mobile Menu">
            <span class="cnvs-hamburger-box"><span class="cnvs-hamburger-inner"></span></span>
          </button>
        </div>

        <!-- Primary Navigation
        ============================================= -->
        <nav class="primary-menu with-arrows">
          <ul class="menu-container">
            <% menus.forEach(menu => { %>
            <li class="menu-item">
              <a class="menu-link" href="<%= menu.slug %>">
                <div><%= menu.name %></div>
              </a>
              <% if (menu.categories && menu.categories.length > 0) { %>
              <ul class="sub-menu-container">
                <% menu.categories.forEach(category => { %>
                <li class="menu-item">
                  <a class="menu-link" href="<%= category.slug %>">
                    <div><%= category.name %></div>
                  </a>
                </li>
                <% }); %>
              </ul>
              <% } %>
            </li>
            <% }); %>
          </ul>
        </nav><!-- #primary-menu end -->

      </div>

    </div>

  </div>
  <div class="header-wrap-clone"></div>

</header>